                     <attDef ident="default" mode="change">
                        <valList mode="add" type="closed">
                           <valItem ident="false"/>
                        </valList>
                     </attDef>
                  </attList>
               </elementSpec>
               <elementSpec ident="settingDesc" mode="change">
                  <attList>
                     <attDef ident="facs" mode="delete"/>
                     <attDef ident="resp" mode="delete"/>
                     <attDef ident="default" mode="change">
                        <valList mode="add" type="closed">
                           <valItem ident="false"/>
                        </valList>
                     </attDef>
                  </attList>
               </elementSpec>
            </schemaSpec>
         </div1>
      </body>
   </text>
</TEI>
